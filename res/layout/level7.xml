        android:padding="1dp"
        android:src="@drawable/fish_02" />

    <ImageButton
        android:id="@+id/fishButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/fishButton2"
