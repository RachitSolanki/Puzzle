        android:padding="1dp"
        android:src="@drawable/fish_04" />

    <ImageButton
        android:id="@+id/fishButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/fishButton1"
