        android:id="@+id/fishButton16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/fishButton15"
        android:layout_toRightOf="@+id/fishButton15"
        android:background="#FFFFFF"
        android:padding="1dp"
        android:src="@drawable/fish_16" />

    <ImageButton
        android:id="@+id/fishButton17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/fishButton16"
        android:layout_below="@+id/fishButton16"
        android:background="#AAAAAA"
        android:padding="1dp"
        android:src="@drawable/fishback" />

</RelativeLayout>