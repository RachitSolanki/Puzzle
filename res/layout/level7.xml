        android:background="#FFFFFF"
        android:padding="1dp"
        android:src="@drawable/fish_01" />

    <ImageButton
        android:id="@+id/fishButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/fishButton1"
